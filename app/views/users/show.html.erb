<div class="pinterest_profile">
  <table>
    <tr>
      <td class="photo">
        <%= image_tag @user.avata.url(:thumb), :class => 'avata' %>
      </td>
    </tr>
    <tr>
     <td>
        <div class="pinterest_username"><%= @user.name %></div>
      </td>
    </tr>
  </table>
</div>

<table class="pins_list">
<% @pins.each do |pin| %>
  <%= render partial: "pin", locals: {pin: pin} %>
<% end %>
</table>